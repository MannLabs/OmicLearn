<?xml version="1.0" encoding="utf-8" standalone="no"?>
<installer-script minSpecVersion="1.000000">
    <title>OmicLearn 1.3.1</title>
    <background mime-type="image/png" file="omiclearn_logo.png" scaling="proportional"/>
    <welcome file="welcome.html" mime-type="text/html" />
    <conclusion file="conclusion.html" mime-type="text/html" />
    <license file="LICENSE.txt"/>
    <options customize="never" allow-external-scripts="no"/>
    <domains enable_localSystem="true" />
    <choices-outline>
        <line choice="OmicLearn"/>
    </choices-outline>
    <choice id="OmicLearn" title="OmicLearn">
        <pkg-ref id="OmicLearn.pkg"/>
    </choice>
    <pkg-ref id="OmicLearn.pkg" auth="Root">OmicLearn.pkg</pkg-ref>
</installer-script>